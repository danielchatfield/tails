<gconfentryfile>

  <entrylist base="/apps/gnome-screensaver">

    <entry>
      <key>lock_enabled</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>lock</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>mode</key>
      <value>
	<string>blank-only</string>
      </value>
    </entry>

    <entry>
      <key>power_management_delay</key>
      <value>
	<int>30</int>
      </value>
    </entry>

  </entrylist>

</gconfentryfile>
