<gconfentryfile>

  <entrylist base="/desktop/gnome">

    <entry>
      <key>applications/browser/exec</key>
      <value>
        <string>iceweasel</string>
      </value>
    </entry>

    <entry>
      <key>background/primary_color</key>
      <value>
	<string>#00006406baba</string>
      </value>
    </entry>

    <entry>
      <key>background/secondary_color</key>
      <value>
	<string>#6666baba0000</string>
      </value>
    </entry>

    <entry>
      <key>background/picture_filename</key>
      <value>
	<string></string>
      </value>
    </entry>

    <entry>
      <key>background/color_shading_type</key>
      <value>
	<string>solid</string>
      </value>
    </entry>

    <entry>
      <key>background/picture_options</key>
      <value>
	<string>none</string>
      </value>
    </entry>

    <entry>
      <key>interface/accessibility</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>interface/menus_have_icons</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>peripherals/keyboard/general/groupPerWindow</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>peripherals/keyboard/kbd/options</key>
      <value>
	<list type="string">
          <value>
            <string>grp	grp:shifts_toggle</string>
          </value>
          <value>
            <string>grp	grp:alt_shift_toggle</string>
          </value>
        </list>
      </value>
    </entry>

    <entry>
      <key>url-handlers/http/command</key>
      <value>
	<string>iceweasel %s</string>
      </value>
    </entry>

    <entry>
      <key>url-handlers/http/needs_terminal</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>url-handlers/https/command</key>
      <value>
	<string>iceweasel %s</string>
      </value>
    </entry>

    <entry>
      <key>url-handlers/https/needs_terminal</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>url-handlers/mailto/command</key>
      <value>
	<string>claws-mail %s</string>
      </value>
    </entry>

    <entry>
      <key>url-handlers/mailto/needs_terminal</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>url-handlers/unknown/command</key>
      <value>
	<string>iceweasel %s</string>
      </value>
    </entry>

    <entry>
      <key>url-handlers/unknown/needs_terminal</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

  </entrylist>

</gconfentryfile>
