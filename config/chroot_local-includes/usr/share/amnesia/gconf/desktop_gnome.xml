<gconfentryfile>

  <entrylist base="/desktop/gnome">

    <entry>
      <key>interface/menus_have_icons</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>peripherals/keyboard/general/groupPerWindow</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>peripherals/keyboard/kbd/options</key>
      <value>
	<list type="string">
          <value>
            <string>grp	grp:shifts_toggle</string>
          </value>
          <value>
            <string>grp	grp:alt_shift_toggle</string>
          </value>
        </list>
      </value>
    </entry>

    <entry>
      <key>url-handlers/unknown/command</key>
      <value>
	<string>iceweasel %s</string>
      </value>
    </entry>

    <entry>
      <key>url-handlers/unknown/needs_terminal</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

  </entrylist>

</gconfentryfile>
