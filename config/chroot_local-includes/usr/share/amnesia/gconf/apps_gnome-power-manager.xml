<gconfentryfile>

  <entrylist base="/apps/gnome-power-manager">

    <entry>
      <key>can_hibernate</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>actions/critical_battery</key>
      <value>
        <string>shutdown</string>
      </value>
    </entry>

    <entry>
      <key>actions/critical_ups</key>
      <value>
        <string>shutdown</string>
      </value>
    </entry>

    <entry>
      <key>backlight/idle_dim_ac</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>buttons/suspend</key>
      <value>
	<string>shutdown</string>
      </value>
    </entry>

    <entry>
      <key>buttons/power</key>
      <value>
	<string>shutdown</string>
      </value>
    </entry>

    <entry>
      <key>buttons/lid_ac</key>
      <value>
	<string>blank</string>
      </value>
    </entry>

    <entry>
      <key>general/can_hibernate</key>
      <value>
	<bool>false</bool>
      </value>
    </entry>

    <entry>
      <key>ui/enable_sound</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

  </entrylist>

</gconfentryfile>
