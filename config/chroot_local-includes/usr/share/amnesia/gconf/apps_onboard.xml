<gconfentryfile>

  <entrylist base="/apps/onboard">

    <entry>
      <key>use_status_icon</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>start_minimized</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>iconify_on_close</key>
      <value>
	<bool>true</bool>
      </value>
    </entry>

    <entry>
      <key>snippets</key>
      <value>
        <list type="string">
	  <value>
	    <string>https://tails.boum.org/</string>
	  </value>
	</list>
      </value>
    </entry>

  </entrylist>

</gconfentryfile>
